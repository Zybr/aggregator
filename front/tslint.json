{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "whitespace": [
      true,
      "check-module"
    ],
    "align": [
      true,
      "parameters",
      "arguments",
      "statements",
      "members",
      "elements"
    ],
    "jsdoc-format": true
  },
  "rulesDirectory": []
}

